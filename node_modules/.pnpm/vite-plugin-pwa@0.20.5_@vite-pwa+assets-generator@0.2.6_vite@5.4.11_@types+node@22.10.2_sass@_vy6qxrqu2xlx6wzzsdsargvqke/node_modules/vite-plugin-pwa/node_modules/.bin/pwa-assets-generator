#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/bin/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/bin/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/@vite-pwa+assets-generator@0.2.6/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/bin/pwa-assets-generator.mjs" "$@"
else
  exec node  "$basedir/../../../../../@vite-pwa+assets-generator@0.2.6/node_modules/@vite-pwa/assets-generator/bin/pwa-assets-generator.mjs" "$@"
fi

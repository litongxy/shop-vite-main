#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules/semver/bin/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules/semver/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules/semver/bin/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules/semver/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules/npm/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/npm@6.14.18/node_modules:/Users/litongb/shop-vite-main1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
